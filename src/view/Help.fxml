<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="400.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.HelpController">
   <children>
      <VBox styleClass="vBox" stylesheets="@../css/myStyle.css" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <TextArea fx:id="helpTextArea" editable="false" styleClass="text-area" stylesheets="@../css/myStyle.css" VBox.vgrow="ALWAYS" />
            <Button mnemonicParsing="false" onAction="#closeButtonClick" prefHeight="25.0" prefWidth="100.0" stylesheets="@../css/myStyle.css" text="Close" />
         </children>
      </VBox>
   </children>
</AnchorPane>
